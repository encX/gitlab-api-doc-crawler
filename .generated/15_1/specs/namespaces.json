      "name": "Get existence of a namespace",
      "description": "Get existence of a namespace by path. Suggests a new namespace path that does not already exist.",
      "resources": [
        {
          "method": "GET",
          "path": "/namespaces/:namespace/exists"
        }
      ],
      "attributes": [
        {
          "name": "namespace",
          "type": "string",
          "required": true,
