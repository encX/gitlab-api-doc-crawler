      "resources": [
        {
          "method": "POST",
          "path": "/projects/:id/access_tokens"
        }
      ],
      "attributes": [
        {
          "name": "id",
          "type": "integer or string",
          "required": true,
          "description": "ID or URL-encoded path of the project"
        },
        {
          "name": "name",
