{
  "origin": "https://docs.gitlab.com/ee/api/packages.html",
  "pageName": "Packages",
  "apis": [
    {
      "name": "Within a project",
      "description": "Although you can filter packages by status, working with packages that have a processing status can result in malformed data or broken packages.",
      "resources": [
        {
          "method": "GET",
          "path": "/projects/:id/packages"
        }
      ],
      "attributes": [
        {
          "name": "id",
          "type": "integer/string",
          "required": true,
          "description": "ID or URL-encoded path of the project"
        },
        {
          "name": "order_by",
          "type": "string",
          "required": false,
          "description": "The field to use as order. One of created_at (default), name, version, or type."
        },
        {
          "name": "sort",
          "type": "string",
          "required": false,
          "description": "The direction of the order, either asc (default) for ascending order or desc for descending order."
        },
        {
          "name": "package_type",
          "type": "string",
          "required": false,
          "description": "Filter the returned packages by type. One of conan, maven, npm, pypi, composer, nuget, helm, terraform_module, or golang. (Introduced in GitLab 12.9)"
        },
        {
          "name": "package_name",
          "type": "string",
          "required": false,
          "description": "Filter the project packages with a fuzzy search by name. (Introduced in GitLab 12.9)"
        },
        {
          "name": "include_versionless",
          "type": "boolean",
          "required": false,
          "description": "When set to true, versionless packages are included in the response. (Introduced in GitLab 13.8)"
        },
        {
          "name": "status",
          "type": "string",
          "required": false,
          "description": "Filter the returned packages by status. One of default (default), hidden, or processing. (Introduced in GitLab 13.9)"
        }
      ],
      "response": [
        {
