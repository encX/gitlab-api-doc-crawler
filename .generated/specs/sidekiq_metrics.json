            "queues": [
              "post_receive",
              "mailers",
              "archive_repo",
              "system_hook",
              "project_web_hook",
              "gitlab_shell",
              "incoming_email",
              "runner",
              "common",
              "default"
            ],
            "labels": [],
            "concurrency": 25,
            "busy": 0
          }
        ],
        "jobs": {
          "processed": 2,
          "failed": 0,
          "enqueued": 0,
          "dead": 0
        }
      }
    }
  ]
}