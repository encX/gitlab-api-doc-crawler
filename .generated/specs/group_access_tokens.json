    },
    {
      "name": "Get a group access token",
      "description": "Get a group access token by ID.",
      "resources": [
        {
          "method": "GET",
          "path": "groups/:id/access_tokens/:token_id"
        }
      ],
      "attributes": [
        {
          "name": "id",
          "type": "integer or string",
          "required": true,
          "description": "ID or URL-encoded path of the group"
