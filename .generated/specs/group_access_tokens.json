    {
      "name": "Create a group access token",
      "description": "Create a group access token.",
      "resources": [
        {
          "method": "POST",
          "path": "groups/:id/access_tokens"
        }
      ],
      "attributes": [
        {
          "name": "id",
          "type": "integer or string",
          "required": true,
          "description": "ID or URL-encoded path of the group"
        },
        {
          "name": "name",
          "type": "String",
          "required": true,
          "description": "Name of the group access token"
        },
        {
          "name": "scopes",
          "type": "Array[String]",
          "required": true,
          "description": "List of scopes"
        },
        {
          "name": "access_level",
          "type": "Integer",
          "required": false,
          "description": "A valid access level. Default value is 40 (Maintainer). Other allowed values are 10 (Guest), 20 (Reporter), and 30 (Developer)."
        },
        {
          "name": "expires_at",
          "type": "Date",
          "required": false,
          "description": "Token expires at midnight UTC on that date"
        }
      ],
      "response": {
        "scopes": [
          "api",
          "read_repository"
        ],
        "active": true,
