{
  "origin": "https://docs.gitlab.com/ee/api/avatar.html",
  "pageName": "Avatar",
  "apis": [
    {
      "name": "Avatar API",
      "description": "",
      "resources": [],
      "attributes": [],
      "response": {}
    },
    {
      "name": "Get a single avatar URL",
      "description": "Parameters:",
      "resources": [
        {
          "method": "GET",
          "path": "/avatar?email=admin@example.com"
        }
      ],
      "attributes": [
        {
          "name": "email",
          "type": "string",
          "required": true,
          "description": "Public email address of the user."
        },
        {
          "name": "size",
          "type": "integer",
          "required": false,
          "description": "Single pixel dimension (because images are squares). Only used for avatar lookups at Gravatar or at the configured Libravatar server."
        }
      ],
      "response": {
        "avatar_url": "https://www.gravatar.com/avatar/e64c7d89f26bd1972efa854d13d7dd61?s=64&d=identicon"
      }
    }
  ]
}