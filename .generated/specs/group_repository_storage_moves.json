    {
      "name": "Schedule a repository storage move for a group",
      "description": "Supported attributes:",
      "resources": [
        {
          "method": "POST",
          "path": "/groups/:group_id/repository_storage_moves"
        }
      ],
      "attributes": [
        {
          "name": "group_id",
          "type": "integer",
