{
  "origin": "https://docs.gitlab.com/ee/api/import.html",
  "pageName": "Import repository from GitHub",
  "apis": [
    {
      "name": "Import API",
      "description": "",
      "resources": [],
      "attributes": [],
      "response": {}
    },
    {
      "name": "Import repository from GitHub",
      "description": "Import your projects from GitHub to GitLab via the API.",
      "resources": [
        {
          "method": "POST",
          "path": "/import/github"
        }
      ],
      "attributes": [
        {
          "name": "personal_access_token",
          "type": "string",
          "required": true,
          "description": "GitHub personal access token"
        },
        {
          "name": "repo_id",
          "type": "integer",
          "required": true,
          "description": "GitHub repository ID"
        },
        {
          "name": "new_name",
          "type": "string",
          "required": false,
          "description": "New repository name"
        },
        {
          "name": "target_namespace",
          "type": "string",
          "required": true,
          "description": "Namespace to import repository into. Supports subgroups like /namespace/subgroup."
        },
        {
          "name": "github_hostname",
          "type": "string",
          "required": false,
          "description": "Custom GitHub Enterprise hostname. Do not set for GitHub.com."
        }
      ],
      "response": {
        "id": 27,
        "name": "my-repo",
        "full_path": "/root/my-repo",
        "full_name": "Administrator / my-repo"
      }
    },
    {
      "name": "Import repository from Bitbucket Server",
      "description": "Import your projects from Bitbucket Server to GitLab via the API.",
      "resources": [
        {
          "method": "POST",
          "path": "/import/bitbucket_server"
        }
      ],
      "attributes": [
        {
          "name": "bitbucket_server_url",
          "type": "string",
          "required": true,
          "description": "Bitbucket Server URL"
        },
        {
          "name": "bitbucket_server_username",
          "type": "string",
          "required": true,
          "description": "Bitbucket Server Username"
        },
        {
          "name": "personal_access_token",
          "type": "string",
          "required": true,
          "description": "Bitbucket Server personal access token/password"
        },
        {
          "name": "bitbucket_server_project",
          "type": "string",
          "required": true,
          "description": "Bitbucket Project Key"
        },
        {
          "name": "bitbucket_server_repo",
          "type": "string",
          "required": true,
          "description": "Bitbucket Repository Name"
        },
        {
          "name": "new_name",
          "type": "string",
          "required": false,
          "description": "New repository name"
        },
        {
          "name": "target_namespace",
          "type": "string",
          "required": false,
          "description": "Namespace to import repository into. Supports subgroups like /namespace/subgroup"
        }
      ],
      "response": {}
    },
    {
      "name": "Automate group and project import",
      "description": "For information on automating user, group, and project import API calls, see Automate group and project import.",
      "resources": [],
      "attributes": [],
      "response": {}
    }
  ]
}