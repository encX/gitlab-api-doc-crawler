              "type": "integer"
            }
          },
          {
            "name": "maven_max_file_size",
            "in": "query",
            "description": "Maximum Maven package file size in bytes.",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "npm_max_file_size",
            "in": "query",
