[
  {
    "/import/github": {
      "post": {
        "summary": "Import repository from GitHub",
        "description": "Import your projects from GitHub to GitLab via the API.",
        "operationId": "importRepositoryFromGithub",
        "parameters": [
          {
            "name": "personal_access_token",
            "in": "query",
            "description": "GitHub personal access token",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "repo_id",
            "in": "query",
            "description": "GitHub repository ID",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "new_name",
            "in": "query",
            "description": "New repository name",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "target_namespace",
            "in": "query",
            "description": "Namespace to import repository into. Supports subgroups like /namespace/subgroup.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "github_hostname",
            "in": "query",
            "description": "Custom GitHub Enterprise hostname. Do not set for GitHub.com.",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "name": {
                      "type": "string"
                    },
                    "full_path": {
                      "type": "string"
                    },
                    "full_name": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  {
    "/import/bitbucket_server": {
      "post": {
        "summary": "Import repository from Bitbucket Server",
        "description": "Import your projects from Bitbucket Server to GitLab via the API.",
        "operationId": "importRepositoryFromBitbucketServer",
        "parameters": [
          {
            "name": "bitbucket_server_url",
            "in": "query",
            "description": "Bitbucket Server URL",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "bitbucket_server_username",
            "in": "query",
            "description": "Bitbucket Server Username",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "personal_access_token",
            "in": "query",
            "description": "Bitbucket Server personal access token/password",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "bitbucket_server_project",
            "in": "query",
            "description": "Bitbucket Project Key",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "bitbucket_server_repo",
            "in": "query",
            "description": "Bitbucket Repository Name",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "new_name",
            "in": "query",
            "description": "New repository name",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "target_namespace",
            "in": "query",
            "description": "Namespace to import repository into. Supports subgroups like /namespace/subgroup",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
]