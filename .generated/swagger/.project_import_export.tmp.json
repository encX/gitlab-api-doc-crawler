    "/projects/{id}/export": {
      "post": {
        "summary": "Schedule an export",
        "description": "Start a new export.",
        "operationId": "scheduleAnExport",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "The ID or URL-encoded path of the project owned by the authenticated user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "description",
            "in": "query",
            "description": "Overrides the project description",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "upload",
            "in": "query",
            "description": "Hash that contains the information to upload the exported project to a web server",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "upload[url]",
            "in": "query",
            "description": "The URL to upload the project",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "upload[http_method]",
            "in": "query",
            "description": "The HTTP method to upload the exported project. Only PUT and POST methods allowed. Default is PUT",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
