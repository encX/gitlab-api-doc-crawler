    "/projects/{id}/clusters/user": {
      "post": {
        "summary": "Add existing cluster to project",
        "description": "Adds an existing Kubernetes cluster to the project.",
        "operationId": "addExistingClusterToProject",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "The ID or URL-encoded path of the project owned by the authenticated user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "name",
            "in": "query",
            "description": "The name of the cluster",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "domain",
            "in": "query",
            "description": "The base domain of the cluster",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "management_project_id",
            "in": "query",
            "description": "The ID of the management project for the cluster",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "enabled",
            "in": "query",
            "description": "Determines if cluster is active or not, defaults to true",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "managed",
            "in": "query",
