    "/projects/{id}/releases/{tag_name}/assets/links": {
      "post": {
        "summary": "Create a link",
        "description": "Create an asset as a link from a Release.",
        "operationId": "createALink",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "The ID or URL-encoded path of the project.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tag_name",
            "in": "path",
            "description": "The tag associated with the Release.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "name",
            "in": "query",
            "description": "The name of the link. Link names must be unique within the release.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "url",
            "in": "query",
            "description": "The URL of the link. Link URLs must be unique within the release.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "filepath",
            "in": "query",
            "description": "Optional path for a Direct Asset link.",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "link_type",
            "in": "query",
            "description": "The type of the link: other, runbook, image, package. Defaults to other.",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "name": {
                      "type": "string"
                    },
                    "url": {
                      "type": "string"
                    },
                    "direct_asset_url": {
                      "type": "string"
                    },
                    "external": {
                      "type": "boolean"
                    },
                    "link_type": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
